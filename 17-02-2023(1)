import java.util.Scanner;
public class hackerrank {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int factorial=1;
        if(n<1 && n>10){
            System.exit(0);
            }
        else{
            for(int i =n;i>1;i--){
                factorial = factorial *i;
            }
        }
        System.out.println(factorial);
        scan.close();     
    }
}
