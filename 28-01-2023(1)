import java.util.*;
public class hackerrank {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int x = scan.nextInt();
        if(x>0){
        System.out.println((double)x);
        }
        else {
        System.out.println("Invalid Input");
        }
        scan.close();    
        }
    }
