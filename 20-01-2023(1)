import java.util.*;
public class solution { 
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);        
        int x = scan.nextInt();
        int y = scan.nextInt();
        if(x<0 | y<0){
            System.out.println("Invalid Input");
        }
        else{
            System.out.println(x +"-"+y);
        }
    }
}
