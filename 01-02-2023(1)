import java.io.*;
import java.util.*;

public class Main
{
  public static void main(String args[])
  {
    Scanner sc = new Scanner(System.in);
    String input;
    input = sc.nextLine();
    char search;
    search = sc.next().charAt(0);
  
  int count=0;
  for(int i=0; i<input.length(); i++)
  {
      if(input.charAt(i) == search)
      count++;
  }
  
  System.out.println(count);
  }
}
