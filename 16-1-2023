import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s= new Scanner(System.in);
        int a= s.nextInt();
        if (a<0)
            System.out.println(-a);
        else
            System.out.println(a) ;  }
}
